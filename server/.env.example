# Google Gemini API Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_google_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Voice Model Configuration
GEMINI_VOICE_MODEL=gemini-2.5-flash-native-audio-preview-12-2025
VOICE_NAME=Aoede
VOICE_SAMPLE_RATE=24000

# Server Configuration
WEBSOCKET_HOST=localhost
WEBSOCKET_PORT=8003
FASTAPI_HOST=0.0.0.0
MEDIA_STREAMS_PORT=8004

# Twilio Configuration (for incoming phone calls)
# Get credentials from: https://console.twilio.com
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_API_SECRET=your_twilio_api_secret

# Twilio Webhook URLs
# For local testing, use ngrok: ngrok http 8004
# Then set TWILIO_WEBHOOK_BASE_URL to your ngrok URL
TWILIO_WEBHOOK_BASE_URL=https://your-ngrok-url.ngrok-free.app

# Media Streams WebSocket Configuration
MEDIA_STREAMS_HOST=your-ngrok-url.ngrok-free.app

# Latency Optimization (for Twilio phone calls)
# Set to "true" to prioritize low latency over interruption prevention
# Set to "false" to prioritize natural conversation (more patient listening)
LOW_LATENCY_MODE=true

# n8n Webhook Configuration (for scheduling meetings)
# This webhook creates calendar events and sends confirmation emails
N8N_WEBHOOK_URL=https://your-n8n-instance.app.n8n.cloud/webhook/schedule-meeting
